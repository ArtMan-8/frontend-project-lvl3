{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "plugin:jest/recommended"],
  "plugins": ["jest", "babel"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2015
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": "error",
    "prettier/prettier": "off"
  }
}
